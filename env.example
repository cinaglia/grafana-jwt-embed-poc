# Grafana Configuration
GRAFANA_BASE_URL=https://your-grafana-instance.com
GRAFANA_TOKEN=your-grafana-token
GRAFANA_DATASOURCE_ID=your-datasource-id
GRAFANA_DASHBOARD_ID=your-dashboard-id

# JWKS Authentication Configuration
# Supported modes: "none", "bearer"
JWKS_AUTH_MODE=none

# Bearer Token Authentication (For testing jwk_set_bearer_token_file)
JWKS_BEARER_TOKEN=secret-token-for-jwks

# JWKS HTTPS Configuration (For testing tls_client_ca)
SERVE_JWKS_VIA_HTTPS=false